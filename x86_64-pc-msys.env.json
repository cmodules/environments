{
  // "$schema": "https://raw.githubusercontent.com/cmodules/environments/main/env.schema.json#",
  "environmentPresets": [
    {
      "name": "x86_64-pc-msys",
      // "version": "0.0.1-a",
      "description": "A set of default environment values for fall-through cases on 'x86_64-pc-msys'.",
      "inherits": [
        "base"
      ],
      "variables": {
        // Binaries
        "AR": "ar",
        "AS": "as",
        "CC": "cc",
        "CPP": "cpp",
        "CXX": "c++",
        "LD": "ld",
        "MT": "mt",
        "NM": "nm",
        "RANLIB": "ranlib",
        "RC": "rc",
        "RM": "rm",
        // Binary flags
        "ARFLAGS": "-rv",
        "ASFLAGS": "",
        "CFLAGS": "",
        "CPPFLAGS": "",
        "CXXFLAGS": "",
        "LDFLAGS": "",
        // Definitions
        "DEFINES": {},
        "DEFINES_DEBUG": {},
        "DEFINES_RELEASE": {},
        // Paths
        "BINDIR": "/usr/bin",
        "INSTALL_BINDIR": "/usr/local/bin",
        "PATH": [
          "/usr/local/bin",
          "/usr/bin",
          "/bin",
          "/c/Windows/System32",
          "/c/Windows",
          "/c/Windows/System32/Wbem",
          "/c/Windows/System32/WindowsPowerShell/v1.0/",
          "/usr/bin/site_perl",
          "/usr/bin/vendor_perl",
          "/usr/bin/core_perl"
        ],
        // Utils
        "EDITOR": "code",
        "SHELL": "bash",
        "PATH_SEPERATOR": ":",
        "SUFFIXES": [
          ".out",
          ".a",
          ".ln",
          ".o",
          ".c",
          ".cc",
          ".C",
          ".cpp",
          ".p",
          ".f",
          ".F",
          ".m",
          ".r",
          ".y",
          ".l",
          ".ym",
          ".yl",
          ".s",
          ".S",
          ".mod",
          ".sym",
          ".def",
          ".h",
          ".info",
          ".dvi",
          ".tex",
          ".texinfo",
          ".texi",
          ".txinfo",
          ".w",
          ".ch",
          ".web",
          ".sh",
          ".elc",
          ".el"
        ]
      }
    }
  ]
}
